{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UseInMemoryDatabase": false,
  "AllowedHosts": "*",
  "OpenIddict": {
    "AuthorizationEndpointUrls": [ "/connect/authorize" ],
    "TokenEndpointUrls": [ "/connect/token" ],
    "UserInfoEndpointUrls": [ "/connect/userinfo" ],
    "DisableTS": false
  },
  "TokenConfig": {
    "Secret": "this is my sample custom Secret key for authentication"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=Auth2Test;Integrated Security=True;"
  },
  "TokenExpiration": 5
}
